using System;
using System.Collections.Generic;

namespace StacksAndQues
{
    class Program
    {
        static void Main(string[] args)
        {
            var input = Console.ReadLine();
            var stack = new Stack<char>();
            for(int i=0; i<input.Length; i++)
            {
                stack.Push(input[i]);
            }
            foreach (var item in stack)
            {
                Console.Write(item);
            }
        }
    }
}
